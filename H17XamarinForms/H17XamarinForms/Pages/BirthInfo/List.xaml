<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="H17XamarinForms.Pages.BirthInfo.List">
    <ContentPage.Content>
        <StackLayout Margin="10,0">
            <Label Text="Patient birth info!"/>

            <Button Command="{Binding CreateCommand}" Text="add new"></Button>
            <ActivityIndicator x:Name="Preloader" IsRunning="true" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"></ActivityIndicator>
            <ListView ItemsSource="{Binding ValueList}" x:Name="ValueList" IsVisible="false">
               
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>